{
  "name": "projeto-controleestoque",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "index.js",
  "scripts": {
  "dev": "nodemon src/index.js",
  "web": "npm run dev --prefix web",
  "db:up": "docker compose up -d",
  "db:down": "docker compose down",
  "wait:db": "wait-on tcp:localhost:5432",
  "up": "npm run db:up",
  "down": "npm run db:down",
  "dev:all": "concurrently -n DB,API,WEB -c green,blue,magenta \"npm:up\" \"npm:api+web\"",
  "api+web": "concurrently -n API,WEB -c blue,magenta \"npm run dev\" \"npm run web\"",
  "dev:ready": "npm run db:up && npm run wait:db && concurrently -n API,WEB -c blue,magenta \"npm run dev\" \"npm run web\"",
  "start": "node src/index.js",
  "prisma": "prisma",
  "seed": "node prisma/seed.js"
},
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.18.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.9.1",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "wait-on": "^9.0.1"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}
